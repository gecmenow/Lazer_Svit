@{
    ViewBag.Title = "Lazer-Svit Лазерная резка и гравировка";
}

@section HeaderMain
{
    <header class="header">
        <div class="header-top">
            <div class="wrapper">
                <div class="header-top-container">
                    <div class="header-contact__info">
                        <a href="https://www.instagram.com/lazersvit/"><i class="fab fa-instagram"></i></a>
                        <a href="mailto:infolazersvit@gmail.com"><i class="fas fa-envelope"></i> infolazersvit@gmail.com</a>
                        <button class="header-popup__toggle"><i class="fas fa-phone"></i> +38 (093) 917-44-89 </button>
                        <div class="header-popup">
                            <p>@Resources.Resource.header_phones</p>
                            <div class="header-popup-container">
                                <div class="header-popup__phone">
                                    <a href="tel:+380939174489"> +38 (093) 917-44-89 </a>
                                    <a href="tel:+380971945130"> +38 (097) 194-51-30 </a>
                                </div>
                                <div class="header-popup__info">
                                    <p>
                                        @Resources.Resource.contact_time_1<br>
                                        @Resources.Resource.contact_time_2
                                    </p>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="lang-container">
                        <a href="/language/ru/"><button class="lang-switcher">RU</button></a>
                        <a href="/language/uk/"><button class="lang-switcher">UA</button></a>
                        <a href="/language/en/"><button class="lang-switcher">ENG</button></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="wrapper">
            <div class="header-middle">
                <button class="menu-toggle">
                    <span></span>
                </button>
                <a class="header__logo" href="/">
                    <img src="~/Content/images/logo.svg" alt="Lazer-Svit Лазерная резка и гравировка" title="Дизайнерская студия  LS">
                </a>
                <nav class="nav">
                    <ul class="nav__list">
                        <li class="nav__item ">
                            <a class="nav__link nav__link--current" href="/">@Resources.Resource.menu_main</a>
                        </li>
                        <li class="nav__item">
                            <a class="nav__link">@Resources.Resource.menu_products <i class="fas fa-angle-down"></i></a>
                            <ul class="nav__item nav__drop-down">
                                @foreach (var category in ViewBag.Category)
                                {
                                    <li class="nav__item"><a class="nav__link" href="/category/@category.UrlName">@category.Name</a></li>
                                }
                            </ul>
                        </li>
                        <li class="nav__item">
                            <a class="nav__link">@Resources.Resource.menu_lazer <i class="fas fa-angle-down"></i></a>
                            <ul class="nav__item nav__drop-down">
                                <li class="nav__item"><a class="nav__link" href="/materials/">@Resources.Resource.menu_lazer_materials</a></li>
                                <li class="nav__item"><a class="nav__link" href="/price/">@Resources.Resource.menu_lazer_price</a></li>
                                <li class="nav__item"><a class="nav__link" href="/info/">@Resources.Resource.menu_lazer_info</a></li>
                            </ul>
                        </li>
                        <li class="nav__item">
                            <a class="nav__link">@Resources.Resource.menu_uv_header <i class="fas fa-angle-down"></i></a>
                            <ul class="nav__item nav__drop-down">
                                <li class="nav__item"><a class="nav__link" href="/uvprinting/">@Resources.Resource.menu_lazer_price</a></li>
                            </ul>
                        </li>
                        <li class="nav__item">
                            <a class="nav__link" href="/feedback/">@Resources.Resource.menu_reviews</a>
                        </li>
                        <li class="nav__item">
                            <a class="nav__link" href="/contacts/">@Resources.Resource.menu_contacts</a>
                        </li>
                    </ul>
                </nav>
                <a href="/cart/" class="shopping-cart">
                    <span class="hopping-cart__counter"></span>
                    <img class="shopping-cart__icon" src="~/Content/images/_cart.png">
                </a>
            </div>
        </div>
        <div class="header-bottom">
            <div id="header-video">
                <img class="video-img" src="~/Content/images/header-video.jpg">
                <video class="video" type="video/mp4" src="~/Content/media/Meanderplatte_HD2.mp4" autoplay="autoplay" loop="loop" muted="muted"></video>
            </div>
            <div class="wrapper">
                <div class="header-video__title">
                    <h1>@Resources.Resource.main_title_h1 <b>@Resources.Resource.main_title_b</b></h1>
                    <h2>@Resources.Resource.main_title_h2</h2>
                </div>
            </div>
        </div>
    </header>
}
